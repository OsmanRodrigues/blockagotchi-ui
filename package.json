{
    "name": "blockagotchi-ui",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "yarn style:lint && yarn vite",
        "build": "vite build",
        "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 5",
        "preview": "vite preview",
        "prettier:check": "prettier src/**/* --check",
        "prettier:fix": "prettier src/**/* --write",
        "style:lint": "yarn lint && yarn prettier:check",
        "gen:gql": "graphql-codegen",
        "gen:tpchain": "typechain --target ethers-v5 --out-dir src/services/rollups/generated './node_modules/@cartesi/rollups/export/artifacts/**/*.json'",
        "gen:all": "yarn gen:gql && yarn gen:tpchain"
    },
    "dependencies": {
        "@cartesi/rollups": "1.0.0",
        "@ethersproject/abi": "5.0.0",
        "@ethersproject/bytes": "^5.0.0",
        "@ethersproject/providers": "5.6.0",
        "@rainbow-me/rainbowkit": "^2.1.2",
        "@tanstack/react-query": "^5.45.0",
        "ethers": "5.7.2",
        "graphql": "^16.8.2",
        "nes.css": "^2.3.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.23.1",
        "urql": "^4.1.0",
        "viem": "2.x",
        "wagmi": "^2.10.2"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^5.0.2",
        "@graphql-codegen/typed-document-node": "^5.0.7",
        "@graphql-codegen/typescript": "^4.0.7",
        "@graphql-codegen/typescript-operations": "^4.2.1",
        "@graphql-codegen/typescript-urql": "^4.0.0",
        "@graphql-typed-document-node/core": "^3.2.0",
        "@typechain/ethers-v5": "10.0.0",
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "@typescript-eslint/eslint-plugin": "^7.2.0",
        "@typescript-eslint/parser": "^7.2.0",
        "@vitejs/plugin-react": "^4.2.1",
        "css-loader": "^7.1.2",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-react": "^7.34.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.6",
        "prettier": "^3.3.2",
        "typechain": "8.0.0",
        "typescript": "^5.2.2",
        "vite": "^5.2.0"
    }
}
